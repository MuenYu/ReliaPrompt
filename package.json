{
    "name": "llm-prompt-testing-tool",
    "version": "1.0.0",
    "description": "Tool for testing agentic server-side prompts against multiple LLMs",
    "main": "dist/server.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/server.js",
        "dev": "ts-node src/server.ts",
        "format": "prettier --write \"src/**/*.ts\" \"public/**/*.{html,js,css}\" \"*.json\"",
        "db:generate": "drizzle-kit generate",
        "db:push": "drizzle-kit push"
    },
    "dependencies": {
        "@aws-sdk/client-bedrock-runtime": "^3.470.0",
        "cors": "^2.8.5",
        "drizzle-orm": "^0.29.3",
        "express": "^4.18.2",
        "openai": "^4.20.1",
        "sql.js": "^1.10.3",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.4",
        "@types/uuid": "^9.0.7",
        "drizzle-kit": "^0.20.10",
        "prettier": "^3.6.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    }
}
